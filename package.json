{
	"name": "mifiel",
	"version": "0.0.1",
	"description": "Mifiel API Client for NodeJS",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"engines": {
		"node": "> 3"
	},
	"config": {
		"crypto_path": "dist/crypto/crypto.js"
	},
	"scripts": {
		"prepare": "yarn run build && yarn run build:cdn",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"test": "jest --forceExit --coverage --verbose",
		"watch-test": "jest --forceExit --coverage --verbose --watch",
		"coveralls": "cat ./coverage/lcov.info | coveralls",
		"build": "tsc",
		"watch-ts": "tsc -w",
		"build:cdn": "npm-run-all --parallel build:cdn:*",
		"build:cdn:crypto": "npm-run-all --parallel crypto:*",
		"crypto:normal": "yarn run bundle -- --entry $npm_package_config_crypto_path --outfile cdn/crypto.js --standalone Crypto",
		"crypto:min": "yarn run bundle -- -p tinyify --entry $npm_package_config_crypto_path --outfile cdn/crypto.min.js --standalone Crypto",
		"bundle": "browserify -t babelify"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Mifiel/nodejs-api-client.git"
	},
	"keywords": [
		"Electronic",
		"document",
		"electronic",
		"signatures",
		"rsa"
	],
	"author": "Genaro Madrid",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Mifiel/nodejs-api-client/issues"
	},
	"homepage": "https://github.com/Mifiel/nodejs-api-client#readme",
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"@types/jest": "^22.1.0",
		"@types/node-forge": "^0.7.11",
		"@types/request": "^2.0.13",
		"babelify": "^10.0.0",
		"browserify": "^16.2.3",
		"coveralls": "^3.0.0",
		"eccrypto": "1.1.0",
		"istanbul": "^0.4.5",
		"jest": "^22.1.4",
		"nock": "^9.1.6",
		"node-forge": "^0.8.0",
		"npm-run-all": "^4.1.5",
		"request": "^2.83.0",
		"tinyify": "^2.5.0",
		"ts-jest": "^22.0.1",
		"ts-node": "^4.1.0",
		"tslint": "^5.9.1",
		"typescript": "^2.6.2"
	},
	"dependencies": {
		"bip32": "^1.0.2"
	},
	"browserify": {
		"transform": [
			[
				"babelify",
				{
					"presets": [
						"@babel/preset-env"
					]
				}
			]
		]
	}
}
